  },
  "dependencies": {
    "async": "~2.1.0",
    "dotenv": "~4.0.0",
    "lodash": "~4.17.0",
    "mkdirp": "~0.5.1",
    "step": "~1.0.0",
    "yargs": "~7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.0",
